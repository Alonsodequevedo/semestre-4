public class Main {
    public static void main(String[] args) {

        int n1 = 1, n2 = 1, n3;
        System.out.print(n1 + " " + n2 + " "); // Imprime los dos primeros términos

        for (int i = 2; i < 10; i++) { // Bucle para calcular los siguientes términos
            n3 = n1 + n2;
            System.out.print(n3 + " ");
            n1 = n2;
            n2 = n3;
        }
    }
}
